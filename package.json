{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "discolink",
  "version": "1.2.1",
  "description": "A javascript library for discord bots to interact with Lavalink",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.ts",
        "default": "./lib/index.js"
      },
      "import": {
        "types": "./lib/index.d.mts",
        "default": "./lib/index.mjs"
      }
    }
  },
  "main": "./lib/index.js",
  "module": "./lib/index.mjs",
  "types": "./lib/index.d.ts",
  "files": [
    "./lib/",
    "NOTICE"
  ],
  "author": "Aman (https://github.com/execaman)",
  "repository": "https://github.com/execaman/discolink",
  "homepage": "https://execaman.github.io/discolink",
  "bugs": "https://github.com/execaman/discolink/issues",
  "keywords": [
    "api",
    "client",
    "library",
    "music",
    "discord",
    "lavalink"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "scripts": {
    "lint": "prettier --check . && tsc --noEmit",
    "build": "tsup",
    "docs": "typedoc",
    "format": "prettier --log-level error --write .",
    "upgrade": "ncu -u && npm update",
    "prepare": "husky || true"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "ws": "^8.18.3"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/ws": "^8.18.1",
    "husky": "^9.1.7",
    "npm-check-updates": "^19.0.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typedoc": "^0.28.13",
    "typedoc-material-theme": "^1.4.0",
    "typedoc-plugin-dt-links": "^2.0.22",
    "typedoc-plugin-mdn-links": "^5.0.9",
    "typescript": "^5.9.3"
  }
}
